{
  "name": "loadeer",
  "version": "1.0.0",
  "description": "Tiny, performant, SEO-friendly lazy loading library",
  "files": [
    "dist"
  ],
  "main": "./dist/loadeer.umd.js",
  "module": "./dist/loadeer.es.js",
  "exports": {
    ".": {
      "import": "./dist/loadeer.es.js",
      "require": "./dist/loadeer.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "echo \"No test specified\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "cleanup": "prettier --write \"src/**/*.ts\"",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "prepare": "husky install",
    "prepublishOnly": "tsc --noEmit && npm run build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/loadeer.git"
  },
  "keywords": [
    "lazy-loading",
    "lazyloading",
    "lazy",
    "observer",
    "lozad.js",
    "lozad"
  ],
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johannschopplich/loadeer/issues"
  },
  "homepage": "https://github.com/johannschopplich/loadeer#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "typescript": "^4.4.2",
    "vite": "^2.5.2",
    "vite-plugin-dts": "^0.7.0"
  },
  "dependencies": {
    "@antfu/utils": "^0.3.0"
  }
}
