{
  "name": "loadeer",
  "version": "1.1.2",
  "description": "Tiny, performant, SEO-friendly lazy loading library",
  "files": [
    "dist"
  ],
  "main": "./dist/loadeer.umd.js",
  "module": "./dist/loadeer.es.js",
  "exports": {
    ".": {
      "import": "./dist/loadeer.es.js",
      "require": "./dist/loadeer.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "echo \"No test specified\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "prepublishOnly": "tsc --noEmit && npm run build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/loadeer.git"
  },
  "keywords": [
    "lazy-loading",
    "lazyloading",
    "lazy",
    "observer",
    "lozad.js",
    "lozad"
  ],
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johannschopplich/loadeer/issues"
  },
  "homepage": "https://github.com/johannschopplich/loadeer#readme",
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.7"
  }
}
