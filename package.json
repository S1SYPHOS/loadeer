{
  "name": "loadeer",
  "version": "1.3.0",
  "description": "Tiny, performant, SEO-friendly lazy loading library",
  "keywords": [
    "lazy-loading",
    "lazyloading",
    "lazy",
    "observer",
    "lozad.js",
    "lozad"
  ],
  "homepage": "https://github.com/johannschopplich/loadeer#readme",
  "bugs": {
    "url": "https://github.com/johannschopplich/loadeer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/loadeer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "exports": {
    ".": {
      "import": "./dist/loadeer.es.js",
      "require": "./dist/loadeer.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js",
    "prepare": "husky install",
    "prepublishOnly": "tsc --noEmit && npm run build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "colorette": "^2.0.16",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "execa": "^5.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.0",
    "prompts": "^2.4.2",
    "semver": "^7.3.5",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  }
}
